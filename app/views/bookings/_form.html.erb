<%= simple_form_for [@office, @booking] do |f| %>
  <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker", id: "range_start" } %>
  <%= f.input :end_date, as: :string, as: :hidden, required: false, input_html: {class: "datepicker", id: "range_end" }  %>
  <%= f.submit "Book", class: "btn btn-primary" %>
<% end %>

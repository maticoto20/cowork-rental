<div class="containers-container">
  <div class="office-container">
    <% if user_signed_in? %>
    <%= link_to "My Bookings (#{current_user.bookings.count})", bookings_path %>
    <%  end %>
    <div class="row">
      <% @offices.each do |office| %>
        <div class="col-xs-6 col-md-6">
          <%= render "offices/card", office: office %>
        </div>
      <% end %>
    </div>
  </div>
  <div id="map" style="width: 30%;" data-markers="<%= @markers.to_json %>" data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
</div>






